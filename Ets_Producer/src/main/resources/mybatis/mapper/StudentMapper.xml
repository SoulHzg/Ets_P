<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ets.producer.mapper.StudentMapper">

    <insert id="addStudent" useGeneratedKeys="true" keyProperty="id" parameterType="com.ets.common.bean.StudentBean">
          insert into student (name,sex,age) values (#{name},#{sex},#{age})
    </insert>

</mapper>
